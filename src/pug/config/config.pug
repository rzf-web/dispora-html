//-------------------------------------------------------------------------------
  @name: Config
  @description: Config wesbite
  -------------------------------------------------------------------------------//

-
  // Suffix Title
  suffixSiteTitle = '- Dispora Jabar';
  suffixSiteDescription = 'Dinas Pemuda dan Olahraga Dispora Provinsi Jawa Barat melalui Bidang Layanan Kepemudaan terus merealisasikan program pembangunan kepemudaan.';
  suffixSiteKeywords = 'Dispora Jabar';

  // Time Refresh
  timeRefresh = 900;

  // Site App Name
  siteAppName = 'dispora';

  // Site Domain
  siteDomain = 'dispora.com';

  // Site URL
  siteURL = 'https://www.dispora.com';

  // Site Theme
  siteTheme = '#086ab9';

  // Author
  author = 'dispora';
  copyright = '2022 dispora. All Rights Reserved';

  // SEO Frienldy
  seoFriendly = true;
  cannonical = 'https://www.dispora.com';

  // OG Facebook
  ogFacebook = {
    enable: true,
    url: 'https://www.facebook.com/dispora',
    type: 'website',
    section: 'home',
    published_time: '2019-04-26 17:35:30T+07:00',
    modified_time: '2019-04-26 17:35:30T+07:00',
    expiration_time: '2019-04-26 17:35:30T+07:00',
    image: 'assets/img/default/og-facebook.jpg',
    imageType: 'image/jpeg'
  };

  // Twitter Card
  twitterCard = {
    enable: true,
    twitterId: '@dispora',
    sizeCard: 'summary_large_image',
    image: 'assets/img/default/twitter-card.jpg'
  };

  // Manifest
  manifest = 'assets/js/data/manifest.json';

  // Icons
  icons = {

    // Web Favicon
    webFavicon: 'assets/img/homescreen/favicon.ico',

    // Add to Home Screen
    homescreen: {

      // Android
      android: {
        s16: 'assets/img/homescreen/favicon-16x16.png',
        s32: 'assets/img/homescreen/favicon-32x32.png',
        s96: 'assets/img/homescreen/favicon-96x96.png',
        s144: 'assets/img/homescreen/android-icon-144x144.png',
        s192: 'assets/img/homescreen/android-icon-192x192.png'
      },

      // Microsoft
      microsoft: {
        s144: 'assets/img/homescreen/ms-icon-144x144.png'
      },

      // iOS
      iOS: {
        default: 'assets/img/homescreen/apple-icon.png',
        s57: 'assets/img/homescreen/apple-icon-57x57.png',
        s60: 'assets/img/homescreen/apple-icon-60x60.png',
        s72: 'assets/img/homescreen/apple-icon-72x72.png',
        s76: 'assets/img/homescreen/apple-icon-76x76.png',
        s114: 'assets/img/homescreen/apple-icon-114x114.png',
        s120: 'assets/img/homescreen/apple-icon-120x120.png',
        s144: 'assets/img/homescreen/apple-icon-144x144.png',
        s152: 'assets/img/homescreen/apple-icon-152x152.png',
        s180: 'assets/img/homescreen/apple-icon-180x180.png'
      }
    }
  };

  //- Social Media
  socialMedia = [
    {
      network: 'youtube',
      url: 'https://www.youtube.com/dispora',
      logo: 'youtube'
    },
    {
      network: 'tiktok',
      url: 'https://www.tiktok.com/dispora',
      logo: 'tiktok'
    },
    {
      network: 'facebook',
      url: 'https://www.facebook.com/dispora',
      logo: 'facebook'
    },
    {
      network: 'twitter',
      url: 'https://twitter.com/dispora',
      logo: 'twitter'
    },
    {
      network: 'instagram',
      url: 'https://www.instagram.com/dispora',
      logo: 'instagram'
    },
  ];

  // schema
  schema = {
    enable: true,
    url: siteURL,
    name: siteAppName,
    logo: siteURL + '/assets/img/logo/dispora.svg',
    contactPoint: {
      telephone: '+62-123-456-7890',
      contactType: 'sales',
      areaServed: 'ID'
    },
    sameAs: [
      socialMedia[0].url,
      socialMedia[1].url,
      socialMedia[2].url,
      socialMedia[3].url,
      socialMedia[4].url
    ],
    search: {
      enable: false,
      url: 'https://www.dispora.com/search/{search_term_string}?src=mc_google'
    }
  };

  //- holdTransitionClass
  holdTransitionClass = 'hold-transition';


  //=======================================================
  //--------------------- ASSETS --------------------------
  //=======================================================

  // CSS
  //--- CSS: Main
  cssMain = 'assets/css/app.min.css';

  //--- JS
  //--- JS: Vendor
  jsVendor = 'assets/js/vendor.min.js';

  //--- JS: Main
  jsMain = 'assets/js/app.min.js';
